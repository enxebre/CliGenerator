{
    "name": "enxebre/CliGenerator",
    "type": "library",
    "description": "CliGenerator is a PHP HTTP library for creating symfony/console a command line user interface dynamically from a given source (.json file, .yaml file, array of information, etc.)",
    "keywords": ["framework", "console", "command", "generator"],
    "homepage": "https://github.com/enxebre/CliGenerator",
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto Garcia Lamela",
            "email": "alberto.garcial@hotmail.com",
            "homepage": "https://github.com/enxebre"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "*"
    },
    "autoload": {
        "psr-0": {
            "CliGenerator\\": "src/",
            "CliGenerator\\Tests\\": "Tests/"

        }
    }
}
